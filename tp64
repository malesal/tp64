#include <iostream>
using namespace std;

char leerCaracterMayuscula();
void determinarTipoCaracter(char c);

int main() {
    int x;
    cout << "Ingresa el número de caracteres: ";
    cin >> x;

    for (int i = 0; i < x; ++i) {
        cout << "Ingresa el carácter " << i + 1 << ": ";
        char caracter = leerCaracterMayuscula();
        determinarTipoCaracter(caracter);
    }

    return 0;
}
char leerCaracterMayuscula() {
    char c;
    cin >> c;
    while ((c < 'A' || c > 'Z') && c != 'Y') {
        cout << "ingresa una letra mayúscula: ";
        cin >> c;
    }
    return c;
}

void determinarTipoCaracter(char c) {
    if (c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U') {
        cout << "V" << endl;
    } else if (c == 'Y') {
        cout << "S" << endl; 
    } else {
        cout << "C" << endl; 
    }
}
